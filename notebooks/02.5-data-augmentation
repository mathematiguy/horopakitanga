  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "ff4a7197-2abe-4380-a7a3-fed0f707e19e",
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('../data/tokens.txt', 'w') as f:\n",
    "    for token in list(token_counts.index):\n",
    "        f.write(f\"{token}\\n\")\n",
    "    f.write(\"[END]\")"
   ]
  },
  {
   "cell_type": "code",
